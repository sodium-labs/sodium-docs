{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "sodium-docs",
    "version": "1.0.0",
    "private": true,
    "description": "The Sodium Labs docs",
    "license": "Apache-2.0",
    "author": "SodiumLabs",
    "homepage": "https://github.com/sodium-labs/sodium-docs#readme",
    "bugs": {
        "url": "https://github.com/sodium-labs/sodium-docs/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sodium-labs/sodium-docs.git"
    },
    "scripts": {
        "dev": "dotenv -- next dev",
        "build": "next build",
        "start": "dotenv -- next start",
        "lint": "next lint",
        "format": "prettier . --write",
        "docs:build": "node docs/install && node docs/build-docs && node docs/patch-docs"
    },
    "dependencies": {
        "dotenv-cli": "^8.0.0",
        "lucide-react": "^0.515.0",
        "next": "15.3.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "typedoc": "^0.28.5",
        "typedoc-material-theme": "^1.4.0",
        "typedoc-plugin-zod": "^1.4.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@stylistic/eslint-plugin": "^4.4.1",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.3",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.12",
        "tailwindcss": "^4",
        "typescript": "^5",
        "typescript-eslint": "^8.34.0"
    }
}
